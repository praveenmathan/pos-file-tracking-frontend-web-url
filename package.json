  "scripts": {
    "clean:dist": "rimraf dist && rimraf coverage",
    "preclean:all": "npm run clean:dist",
    "clean:all": "npm cache clean --force && rimraf node_modules",
    "test": "jest --config=jest.config.js",
    "prebuild": "npm run clean:dist",
    "build:server": "webpack  -p --progress --env.server",
    "build": "webpack -p --config ./webpack.config.js --progress --env.server",
    "deploy:client": "upload to s3",
    "start:server:local": "node ./dist/server.js",
    "start": "nodemon --exec npm run start:nodemon",
    "start:nodemon": "webpack --progress --env.server && npm run start:server:local",
    "create:lambda": "cross-var \"rimraf $npm_package_name-$npm_package_version.zip && website-deploy lambda build --path lambda.js --path dist --include-node-modules --debug $npm_package_name-$npm_package_version.zip\"",
    "preupload": "npm run build && npm run create:lambda",
    "upload": "cross-var \"website-deploy lambda update demo-web-app $npm_package_name-$npm_package_version.zip --debug --profile poc",
    "invalidate:cf": "website-deploy invalidate-cache E2OQRUX4DSAW0K --profile poc",
    "deploy": "npm run upload && npm run invalidate:cf"
  },
